.version 6.5
.target sm_30
.address_size 64

.visible .entry slct(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .u32 	    temp1;
    .reg .u32 	    temp2;
    .reg .s32 	    temp3;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.u32          temp1, [in_addr];
    ld.u32          temp2, [in_addr + 4];
    ld.s32          temp3, [in_addr + 8];
    slct.u32.s32   	temp1, temp1, temp2, temp3;
    st.u32          [out_addr], temp1;
	ret;
}
